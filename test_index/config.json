{
  "seed": 42,
  "enable_deterministic": true,
  "model_name": "resnet50",
  "pretrained": true,
  "embedding_dim": 2048,
  "image_size": 224,
  "batch_size": 16,
  "normalize_embeddings": true,
  "image_channels": 3,
  "min_image_size": 50,
  "max_image_size": 4000,
  "check_image_quality": true,
  "reject_blank_images": true,
  "blank_threshold": 0.05,
  "entropy_threshold": 1.0,
  "duplicate_threshold": 0.92,
  "use_union_find": true,
  "anomaly_k_neighbors": 5,
  "anomaly_top_n": 8,
  "num_workers": 4,
  "use_memory_mapping": true,
  "cache_embeddings": true,
  "output_format": "json",
  "include_metadata": true,
  "verbose": true,
  "use_phi_safe_logging": true,
  "anonymize_filenames": true,
  "index_dir": null,
  "assets_dir": null,
  "output_dir": null
}